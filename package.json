{
  "name": "jsxls",
  "version": "0.1.13-alpha.0",
  "description": "JavaScript realizes all functions of Excel 2016",
  "main": "index.js",
  "scripts": {
    "bootstrap": "node scripts/bootstrap.js",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "create-api-doc": "api-documenter markdown --input-folder ./temp --output-folder ./temp/markdown",
    "test": "jest",
    "release": "node scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cz": "conventional-changelog -p atom -i CHANGELOG.md -s -r 0 && git add . && git status && git cz",
    "push": "npm version patch && npm publish .  && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kongnet/excel.git"
  },
  "keywords": [
    "excel",
    "javascript",
    "es6",
    "nodejs"
  ],
  "author": "Kongnet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kongnet/excel/issues"
  },
  "homepage": "https://github.com/kongnet/excel#readme",
  "dependencies": {
    "meeko": "^1.8.103"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.8.16",
    "@microsoft/api-extractor": "^7.8.14",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^26.0.3",
    "brotli": "^1.3.2",
    "conventional-changelog-cli": "^2.0.34",
    "cz-jt": "^0.1.11",
    "enquirer": "^2.3.5",
    "execa": "^4.0.2",
    "jest": "^26.1.0",
    "rollup": "^2.18.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-jt"
    }
  }
}
